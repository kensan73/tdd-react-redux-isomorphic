{
  "name": "redux-search-application",
  "version": "1.0.0",
  "description": "Sample Redux Isomorphic Search Application.",
  "main": "src/server.js",
  "scripts": {
    "lint": "esw src test -c .eslintrc --ext .js --ext .jsx",
    "build:semantic": "cd semantic && gulp build",
    "report": "babel-node ./node_modules/.bin/babel-istanbul cover _mocha -- --reporter mochawesome",
    "start": "nodemon server.bundle.js",
    "dev": "npm-run-all --parallel lint:watch build:dev:server build:dev:client start",
    "build:dev": "webpack --config webpack/dev/webpack.server.config.js && webpack --config webpack/dev/webpack.config.js",
    "build:dev:server": "webpack --config webpack/dev/webpack.server.config.js --watch",
    "build:dev:client": "webpack --config webpack/dev/webpack.config.js --watch",
    "test": "mocha --reporter list",
    "test:watch": "npm run test -- --watch",
    "lint:watch": "npm run lint -- --watch",
    "build": "npm run lint && npm run build:prod:server && npm run build:prod:client && npm run build:semantic && mocha --reporter mochawesome",
    "build:prod:server": "webpack --config webpack/prod/webpack.server.config.js",
    "build:prod:client": "webpack --config webpack/prod/webpack.config.js",
    "test:pipeline": "npm run coverage",
    "mountebank": "mb --configfile src/mocks/imposters.ejs --allowInjection"
  },
  "author": "Hany Elemary",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "body-parser": "^1.15.0",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "deep-equal": "^1.0.1",
    "express": "^4.13.4",
    "http-status-codes": "^1.0.6",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.5.2",
    "redux-logger": "^2.7.4",
    "redux-saga": "^0.14.0",
    "scroll-behavior": "^0.9.2",
    "semantic-ui": "=2.2.10",
    "semantic-ui-visibility": "^2.2.1",
    "svg-url-loader": "^1.0.3"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-istanbul": "^0.12.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.18.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "eslint-watch": "^2.1.11",
    "expect": "^1.20.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "gulp": "^3.9.1",
    "jsdom": "^9.9.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.2.0",
    "mochawesome": "^2.0.0",
    "mountebank": "^1.7.2",
    "nock": "^9.0.2",
    "node-libs-browser": "^2.0.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.0",
    "path": "^0.12.7",
    "react-addons-test-utils": "^15.4.1",
    "redux-mock-store": "^1.1.2",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-node-externals": "^1.5.4"
  }
}
